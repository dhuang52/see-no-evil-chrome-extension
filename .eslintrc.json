{
    "env": {
        "browser": true,
        "es2021": true,
        "webextensions": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "jsx-quotes": [2, "prefer-single"],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    }
}
